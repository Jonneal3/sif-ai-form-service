      "FormRequest": {
        "additionalProperties": true,
        "description": "Accepts both the native service shape and the sif-widget new-batch shape.\n\nNative shape uses `batchId`, `stepDataSoFar`, `alreadyAskedKeys`, optional `batchPolicy`.\nWidget shape uses `{ session, currentBatch, state: { answers, askedStepIds, formPlan }, request }`.",
        "properties": {
          "alreadyAskedKeys": {
            "items": {
              "type": "string"
            },
            "title": "Alreadyaskedkeys",
            "type": "array"
          },
          "batchId": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Batchid"
          },
          "batchNumber": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Batchnumber"
          },
          "batchPolicy": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Batchpolicy"
          },
          "currentBatch": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/CurrentBatch"
              },
              {
                "type": "null"
              }
            ]
          },
          "maxSteps": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Maxsteps"
          },
          "request": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/RequestFlags"
              },
              {
                "type": "null"
              }
            ]
          },
          "session": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/SessionInfo"
              },
              {
                "type": "null"
              }
            ]
          },
          "state": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/WidgetState"
              },
              {
                "type": "null"
              }
            ]
          },
          "stepDataSoFar": {
            "additionalProperties": true,
            "title": "Stepdatasofar",
            "type": "object"
          }
        },
        "title": "FormRequest",
        "type": "object"
      },
      "FormResponse": {
        "additionalProperties": true,
        "description": "Response for `POST /v1/api/form`.\n\nNotes:\n- `formPlan` may be returned to bootstrap the session when the request is a \"new batch\"\n  request and no plan was provided by the client.",
        "properties": {
          "formPlan": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Formplan"
          },
          "miniSteps": {
            "items": {
              "additionalProperties": true,
              "type": "object"
            },
            "title": "Ministeps",
            "type": "array"
          },
          "requestId": {
            "title": "Requestid",
            "type": "string"
          },
          "schemaVersion": {
            "default": "0",
            "title": "Schemaversion",
            "type": "string"
          }
        },
        "required": [
          "requestId"
        ],
        "title": "FormResponse",
        "type": "object"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },

# Supabase (required for most requests)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# DSPy (LLM provider)
DSPY_PROVIDER=groq
GROQ_API_KEY=
# DSPY_PROVIDER=openai
# OPENAI_API_KEY=

# Optional
DSPY_MODEL_LOCK=
DSPY_TEMPERATURE=0.7
DSPY_LLM_TIMEOUT_SEC=20
DSPY_NEXT_STEPS_MAX_TOKENS=2000
DSPY_IMAGE_PROMPT_MAX_TOKENS=900

# Optional per-module overrides (Planner vs Renderer)
# If unset, modules fall back to the global DSPY_* values above.
DSPY_PLANNER_PROVIDER=
DSPY_PLANNER_MODEL_LOCK=
DSPY_PLANNER_MODEL=
DSPY_PLANNER_TEMPERATURE=
DSPY_PLANNER_TIMEOUT_SEC=
DSPY_PLANNER_MAX_TOKENS=

DSPY_RENDERER_PROVIDER=
DSPY_RENDERER_MODEL_LOCK=
DSPY_RENDERER_MODEL=
DSPY_RENDERER_TEMPERATURE=
DSPY_RENDERER_TIMEOUT_SEC=
DSPY_RENDERER_MAX_TOKENS=

# Pipeline toggles
AI_FORM_RENDER_CACHE=false
AI_FORM_RENDER_CACHE_TTL_SEC=600
AI_FORM_LOG_LATENCY=false

# Image generation
IMAGE_PROVIDER=mock
